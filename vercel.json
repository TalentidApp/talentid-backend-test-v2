{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js",
        "headers": {
          "Access-Control-Allow-Origin": ["http://localhost:3000/","https://offers.talentid.app/","http://localhost:5173/"]
        }
      }
    ]
  }

  
  